{
  "name": "zhitoujianli",
  "version": "1.0.0",
  "description": "智投简历 - AI智能求职助手",
  "build": {
    "command": "npm run build",
    "output": "build"
  },
  "routes": [
    {
      "path": "/api/*",
      "target": "https://zhitoujianli.com/api"
    },
    {
      "path": "/*",
      "target": "index.html"
    }
  ],
  "headers": {
    "/*": {
      "X-Frame-Options": "DENY",
      "X-Content-Type-Options": "nosniff",
      "X-XSS-Protection": "1; mode=block"
    }
  },
  "redirects": [
    {
      "from": "/admin",
      "to": "https://zhitoujianli.com/admin"
    }
  ]
}
